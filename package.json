{
  "name": "react-flickr-gallery",
  "version": "1.0.0",
  "description": "react flickr gallery component",
  "main": "src/components/gallery.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/aganglada/react-flickr-gallery.git"
  },
  "author": "Alejandro Garcia Anglada <aganglada@gmail.com> (http://aganglada.com)",
  "license": "MIT",
  "homepage": "https://bitbucket.org/aganglada/react-flickr-gallery#readme",
  "dependencies": {
    "autoprefixer": "6.4.0",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-istanbul": "1.0.3",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-runtime": "6.11.6",
    "chokidar-cli": "1.2.0",
    "chunk-manifest-webpack-plugin": "0.1.0",
    "clean-css": "3.4.19",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-polyfill": "0.8.2",
    "file-loader": "0.9.0",
    "html-minify-loader": "1.1.0",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "node-libs-browser": "1.0.0",
    "node-sass": "3.10.0",
    "null-loader": "0.1.1",
    "postcss-cli": "2.5.2",
    "postcss-loader": "0.9.1",
    "postcss-url": "5.1.2",
    "raw-loader": "0.5.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-hot-loader": "1.3.0",
    "resolve-url-loader": "1.6.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-manifest-plugin": "1.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "eslint": "4.3.0",
    "eslint-plugin-react": "7.1.0",
    "webpack-dev-server": "1.16.1"
  },
  "engines": {
    "node": ">=4.2.1",
    "npm": "~3.10.7"
  },
  "scripts": {
    "sass": "node-sass --output-style compressed ./src/styles/app.scss",
    "sass:dev": "npm run sass -- --follow --output ./build --source-map true",
    "sass:prod": "npm run sass -- --output ./build",
    "sass:watch": "npm run sass:dev && npm run postcss:dev && chokidar './src/**/*.scss' -c 'npm run sass:dev && npm run postcss:dev'",
    "postcss:dev": "npm run postcss -- --replace ./build/app.css",
    "postcss:prod": "npm run postcss -- --replace ./build/app.css",
    "postcss": "postcss -c ./config/postcss.json",
    "prod": "npm run build && npm run sass:prod && npm run postcss:prod",
    "dev": "npm run sass:dev && npm run sass:watch | npm run webpack:dev -- --watch --inline --debug --colors",
    "build": "NODE_ENV=production webpack --config ./webpack/config.js --optimize-minimize --optimize-dedupe --profile",
    "webpack:dev": "webpack-dev-server --config ./webpack/config.js --content-base ./ --watch --hot --inline --progress",
    "lint:js": "eslint src/ --ext .js --ignore-pattern *.json"
  },
  "bin": {
    "chokidar": "node_modules/.bin/chokidar",
    "node-sass": "node_modules/.bin/node-sass",
    "phantomjs": "node_modules/.bin/phantomjs",
    "webpack": "node_modules/.bin/webpack",
    "webpack-dev-server": "node_modules/.bin/webpack-dev-server"
  }
}
